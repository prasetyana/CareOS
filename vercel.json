{
    "rewrites": [
        {
            "source": "/:path*",
            "destination": "/:path*"
        }
    ],
    "headers": [
        {
            "source": "/:path*",
            "headers": [
                {
                    "key": "X-Tenant-Host",
                    "value": "$host"
                }
            ]
        }
    ]
}