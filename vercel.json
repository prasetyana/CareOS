{
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "headers": [
        {
            "source": "/:path*",
            "headers": [
                {
                    "key": "X-Tenant-Host",
                    "value": "$host"
                }
            ]
        }
    ]
}